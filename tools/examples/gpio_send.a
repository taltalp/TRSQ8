LDL 255 # set gpio all input
ST 133 # TRIS
LD 134 # IGPIO Load
ST 10 # save GPIO data
LDL 0 # set spi module
ST 0x80 # 
LD 10 # load GPIO data
ST 0x82 # set spi tx data
LDL 0x10 # set spi enable
ST 0x80
LDL 1 # busy check mask
AND 0x80 # check busy
SKZ
GOTO 10# still busy
GOTO 20# send finished
NOP
NOP
NOP
NOP
NOP
LDL 1 # mask
ST 15 # save mask
LDL 16 # IIC TXDATA (0001000 0)
ST 146 # IICTX
LDL 8 # IIC start
ST 144 # IICCON
LDL 0 # clear start flag
ST 144
LD 144 # load IICCON
AND 15 # IICCON & 0x01 (busy)
SKZ
GOTO 28 # still busy
GOTO 40 # send finished
NOP
NOP
NOP
NOP
NOP
NOP
NOP
LD 10 # TXDATA (GPIO DATA)
ST 146 # IICTX
LDL 8 # IIC start
ST 144 # IICCON
LDL 0 # clear start flag
LD 144 # load IICCON
AND 15
SKZ
GOTO 45 # still busy
GOTO 60 # send finished
NOP
NOP
NOP
NOP
NOP
NOP
NOP
NOP
NOP
NOP
LDL 16 # stop flag
ST 144 # IICCON
NOP
LDL 0 # clear stop flag
ST 144
GOTO 0 # restart
HALT
